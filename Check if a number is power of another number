#include <iostream>
#include <cmath>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    long long int n,m,i,c=0;
	    cin>>n>>m;
	    for(i=0;i<=m;i++)
	    {
	        if(pow(n,i) == m) { cout<<1<<endl; break;}
	    }
	    if(i>m) cout<<0<<endl;
	         
	    
	}
	return 0;
}
