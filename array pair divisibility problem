#include <bits/stdc++.h>
using namespace std;

void isPairSumDivisibility(int s, int n, int k){
    if(s%k ==0 && n%2 ==0) cout<<"True\n";
    else cout<<"False\n";
}
int main() {
    int t; cin>>t; while(t--){
        int n,k,s=0; cin>>n; int a[n]; 
        for(int i=0; i<n; i++) {
            cin>>a[i];
            s=s+a[i];
        }
        cin>>k;
        isPairSumDivisibility(s,n,k);
    }
	return 0;
}
