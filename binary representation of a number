#include <iostream>
using namespace std;

int main() {
	int t; cin>>t;
	while(t--)
	{
	    int n; cin>>n;
	    int bin[14],i=0;
	    for(int k=0;k<14;k++)bin[k]=0;
	    while(n)
	    {
	        bin[i]=n%2;
	        n/=2;
	        i++;
	    }
	    for(int j=13; j>=0; j--)
	     cout<<bin[j];
	cout<<endl;
	    
	}
	
	return 0;
}
