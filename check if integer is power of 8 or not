#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    ll n,f=0; cin>>n;
	    if(floor(log2(n)) == ceil(log2(n))){
	    int k= log2(n);
	    if(k%3 == 0)cout<<"Yes\n";
	    else cout<<"No\n";
	    }
	    else cout<<"No\n";
	}
	return 0;
}
