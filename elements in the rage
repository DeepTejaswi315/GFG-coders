#include <bits/stdc++.h>
#define f for(int i=0; i<n; i++)
#define ll long long int
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    int n,a,b,c=0; cin>>n>>a>>b;
	    ll x; 
	    f{
	        cin>>x;
	        if(x>=a && x<=b) c++;
	    }
	    c==(b-a)+1 ? cout<<"Yes\n" : cout<<"No\n";
	}
	return 0;
}
