import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner sc= new Scanner(System.in);
		int t = sc.nextInt();
		while(t-- > 0)
		{
		    int n=sc.nextInt();
		    int a[] = new int[n];
		    int i,m;
		    for(i=0;i<n;i++) a[i]=sc.nextInt();
		    m=a[0];
		    for(i=1;i<n;i++)
		    {
		     m=m^a[i];
		    }
		    System.out.println(m);
		}
	}
}
