#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    int n; cin>>n;
	    int a[n],flag=0;
	    for(int i=0; i<n; i++){
	        cin>>a[i];
	    }
	    flag=a[0];
	    for(int i=1; i<n; i++){
	        flag=flag^a[i];
	    }
	    cout<<flag<<endl;
	}
	return 0;
}
