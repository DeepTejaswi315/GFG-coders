#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    string s; cin>>s;
	    int even=0,odd=0;
	    for(int i=0; i<s.size(); i++){
	        if(s[i]=='1'){
	            if((i+1)%2 == 0) even++;
	            else odd++;
	        }
	    }
	  int k = abs(odd-even);
	   //cout<<even<<" "<<odd<<" "<<k<<endl;
	   if(k%3 == 0) cout<<'1'<<endl;
	   else cout<<'0'<<endl;
	}
	return 0;
}
