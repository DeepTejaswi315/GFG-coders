#include <bits/stdc++.h>
#define f for(int i=0; i<n; i++)
#define ll long long int
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    ll n,prod=0; cin>>n;
	    int a[n], b[n];
	    f cin>>a[i];  f cin>>b[i];
	    sort(a,a+n);
	    sort(b,b+n);
	    f{
	        prod+= (a[i]*b[n-i-1]);
	    }
	    cout<<prod<<'\n';
	}
	return 0;
}
